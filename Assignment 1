#include <stdio.h>
#include <string.h>

int main() {

    
    char a[1001],i;
    int j,c;
    scanf("%s",&a);
    for(i=48;i<=57;i++)
    {
        c=0;
        for(j=0;j<strlen(a);j++)
        {
            if(a[j]==i)
            {
                c++;
            }
        }
        printf("%d ",c);
    }    
    return 0;
}
